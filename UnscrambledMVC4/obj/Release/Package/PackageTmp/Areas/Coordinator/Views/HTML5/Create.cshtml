@model Unscrambled.Areas.Coordinator.Models.Game

@{
    Layout = null;
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<header><h1>Demo Forms</h1></header>
<form>
    <input type="text" autofocus name="demo" required placeholder="name" title="name" value="@Model.Name" />
    <br />
    <input type="email" name="demoemail" required placeholder="name@domain.com" title="name@domain.com" />
    <br />
    <input type="url" name="demourl" placeholder="http://www.yoursite.com" title="URL must begin with http://" />
    <br />
    <meter id="meterofdaysinmonth" min="1" max="31" value="5">5th Day in Month</meter>
    <input type="submit" />
</form>
