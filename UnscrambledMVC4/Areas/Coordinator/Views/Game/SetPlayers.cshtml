@model Unscrambled.Areas.Coordinator.Models.ViewModels.SetPlayersPageView

@{
    ViewBag.Title = "Players";
}


@{
    Html.RenderPartial("~/Areas/Coordinator/Views/Shared/PageTop.cshtml");
    if (Model.setPlayersVM.Game.IsHandicapped == "Y")
    {
        Html.RenderPartial("~/Areas/Coordinator/Views/Shared/_SetPlayers.cshtml");
    }
    else
    {
        Html.RenderPartial("~/Areas/Coordinator/Views/Shared/_SetPlayersNoCaps.cshtml");
    }
    Html.RenderPartial("~/Areas/Coordinator/Views/Shared/PageLeftNavAndBottom.cshtml", Model.leftNavVM);

}
